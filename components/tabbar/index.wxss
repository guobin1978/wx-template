.tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  box-shadow: 0 -1px 1px 0 #e7e3e3;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 20rpx;
}
.tabbar-item {
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
}
.tabbar-image {
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 6rpx;
}
